{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "webiot": "http://schema.webizing.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "webiot:EnergyApplianceMonitor",
      "@type": "rdfs:Class",
      "rdfs:comment": "\n            EnergyApplianceMonitor Sensor - 공기질 로깅 ex)Foobot(5분 주기로 데이터를 가져옴)\n        ",
      "rdfs:label": "EnergyApplianceMonitor",
      "schema:sameAs": {
        "@id": "webiot:EnergyApplianceMonitor"
      }
    },
    {
      "@id": "webiot:applianceId",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        가전제품 Id\n                    ",
      "rdfs:label": "\n                            applianceId\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:location",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        location of the sensor - format: free\n                    ",
      "rdfs:label": "\n                            location\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:activePower",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        appliance activePower - unit: W, 양수: On, 음수: Off\n                    ",
      "rdfs:label": "\n                            activePower\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Float"
      }
    },
    {
      "@id": "webiot:address",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        building where the sensor resides - format: organization-building ex)yongjaelee-house, kist-l1, kist-l8\n                    ",
      "rdfs:label": "\n                            address\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:room",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        room number of the building where the sensor resides - ex) L8321\n                    ",
      "rdfs:label": "\n                            room\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:user",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        user of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm\n                    ",
      "rdfs:label": "\n                            user\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "webiot:time",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        time at which the data measurement was made - ex)\"2017-05-30T18:54:20+09:00\"\n                    ",
      "rdfs:label": "\n                            time\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:DateTime"
      }
    },
    {
      "@id": "webiot:name",
      "@type": "rdf:Property",
      "rdfs:comment": "\n                        name of the sensor - format: SensorXX ex)Foobot00, Beddit02\n                    ",
      "rdfs:label": "\n                            name\n                        ",
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    }
  ]
}